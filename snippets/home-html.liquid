<!-- collection products slider section start -->
<section class="collection-product-section shopify-section">
  <div class="container">
    <div class="section-heading center-heading">
      <h2>Collection Trending Product</h2>
    </div>
    <div class="yv-tabs-wrapper">
      <ul class="yv-tabs">
        <li class="yv-tab-item active"><a href="javascript:void(0)" class="yv-tab-link">Featured</a></li>
        <li class="yv-tab-item"><a href="javascript:void(0)" class="yv-tab-link">Best Sellers</a></li>
        <li class="yv-tab-item"><a href="javascript:void(0)" class="yv-tab-link">New Arrivals</a></li>
      </ul>
      <div class="yv-tab-content">
        <div class="yv-tab-pane">
          <div class="yv-featured-product-grid">
            <div class="row">
              <div class="col-6 col-sm-6 col-md-4 col-lg-3">
                <div class="yv-product-card">
                  <div class="yv-product-card-img has-multiimage ">
                    <a class="yv-product-img" href="#">
                      <img src="//cdn.shopify.com/s/files/1/0597/9668/5009/products/watch.png?v=1644838541" alt="product image" />
                      <img class="product-second-img" src="//cdn.shopify.com/s/files/1/0597/9668/5009/products/watch.png?v=1644838541" alt="product image" />
                    </a>
                    <ul class="yv-product-options">
                      <li>
                        <a href="#">
                          <img src="https://cdn.shopify.com/s/files/1/0597/9668/5009/files/cart.png?v=1645011498" alt="cart icon" />
                        </a>
                      </li>
                      <li>
                        <a href="#">
                          <img src="https://cdn.shopify.com/s/files/1/0597/9668/5009/files/cart.png?v=1645011498" alt="cart icon" />
                        </a>
                      </li>
                    </ul>      
                    <div class="similar_options" data-url="{{product.url}}">
                      <span class="vs-icon text-{{settings.show_similar_text}}">
                        {%- render 'similar-product-icon' -%}
                        <span class="similar-text">{{'products.product.similar_products' | t}}</span>
                      </span>
                    </div>
                    <div class="yv-product-badge sold-badge">
                      Sold Out
                    </div> 
                  </div>
                  <div class="yv-product-information">
                    <div class="ratingbox" {{ block.shopify_attributes }}>
                      <span class="shopify-product-reviews-badge" data-id="{{ product.id }}"></span>
                    </div>
                     <a href="#" class="yv-product-title" title="">Full Sleeve Solid Women</a>
                    <div class="yv-prizebox" data-saved="{{settings.show_saved_amount}}"  data-saved-style="{{settings.saved_amount_style}}" data-price-wrapper=""   {{ block.shopify_attributes }}>
                      <span class="yv-product-price">$1,100.00</span>
                      <span class="yv-product-compare-price"> $1,190.00</span>
                      <span class="yv-product-percent-off"> 50% Off</span>
                    </div>
                  </div>
                </div>
              </div>  
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- collection products slider section end -->