
:root {
    --black: #000000;
    --white:#ffffff;
    --gray: #ececec;
    --dark-gray: #7b7b7b;
    --theme-color: #c58b2f;
    --green:#6cad61;
    --red:#ec7c7c;
    --font-family:'{{ settings.type_header_font_family.family }}',{{ settings.type_header_font_family.fallback_families }};
    --extra-small:12px;
    --small-text: 13px;
    --reg-text: 14px;
    --med-text: 16px;
    --large-text: 18px;
    --thin:100;
	--light:300;
	--normal:400;
	--medium:500;
	--semibold:600;
  	--bold:700;
  	--extrabold:900;    }

