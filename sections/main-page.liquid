<div class="top_page_title no-bg">
  <div class="container">
    <div class="contact-banner-text">
      <div class="banner_title">
        <h1>{{page.title}}</h1>
      </div>
    </div>
  </div>
</div>
<div class="container page-description">
  <div class="inner-container">
    {{page.content}}
  </div>
</div>

{% schema %}
{
  "name": "t:sections.main-page.name",
  "tag": "section",
  "class": "banner-top-section",
  "settings": [
    {
      "type": "color",
      "id": "bgcolor",
      "label": "Background Color"
    }
  ]
}
{% endschema %}
